# 서버 설정
PORT=3000
NODE_ENV=production

# 데이터베이스 설정
DB_HOST=localhost
DB_PORT=5432
DB_NAME=express_db
DB_USER=postgres
DB_PASSWORD=your_password_here

# Redis 설정
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# API 보안
API_KEY=your_api_key_here

# 로깅 설정
LOG_LEVEL=info
LOG_FORMAT=json

# 캐시 설정
CACHE_TTL=60

# CORS 설정
CORS_ORIGIN=http://localhost:3000

# 레이트 리밋
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100
